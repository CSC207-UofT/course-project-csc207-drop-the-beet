<AnchorPane fx:id="todoListAnchorPane" prefHeight="521.2" prefWidth="648.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ui.planner.SettingsViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="93.0" prefWidth="273.0" text="Change Account Settings">
         <font>
            <Font name="Times New Roman Bold" size="21.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="107.0" prefHeight="45.0" prefWidth="119.0" text="User Name">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="userNameText" layoutX="28.0" layoutY="151.0" prefHeight="30.0" prefWidth="246.0" />
      <Label layoutX="350.0" layoutY="107.0" prefHeight="45.0" prefWidth="119.0" text="Email">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="emailText" layoutX="348.0" layoutY="152.0" prefHeight="30.0" prefWidth="246.0" />
      <Label layoutX="28.0" layoutY="202.0" prefHeight="45.0" prefWidth="119.0" text="New Password">
         <font>
            <Font name="Times New Roman" size="13.0" />
